                                        <div data-bind="validationElement:CompanyProfileId" class="col-sm-3">
                                            <select class="form-control" data-bind="options:CompanyProfileList,
                                                    optionsValue : 'Id' ,
                                                    optionsText:'Name',
                                                    optionsCaption:'--Select--',
                                                    value:CompanyProfileId
                                                    "></select>
                                        </div>                                      
                                    </div>

