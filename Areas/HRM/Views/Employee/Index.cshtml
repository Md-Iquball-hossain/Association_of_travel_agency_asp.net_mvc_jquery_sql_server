                                            <div class="col-sm-4 col-sm-offset-1">
                                                <label for="DivisionId" class="control-label">Division<span class="required">*</span> :</label>
                                            </div>
                                            <div class="col-sm-6" data-bind="validationElement:PresentAddress.DivisionId">
                                                <select class="form-control" id="division" data-bind="
                                                  options:PresentAddress.DivisionList,
                                                  optionsValue: 'Id',
                                                  optionsText:'Name',
                                                  optionsCaption:'--Select--',
                                                  value:PresentAddress.DivisionId
                                                "></select>
                                            </div>

                                        </div>
                                        <div class="row row_design">
                                            <div class="col-sm-4 col-sm-offset-1">
                                                <label for="DistrictId" class="control-label">District<span class="required">*</span> :</label>
                                            </div>
                                            <div class="col-sm-6" data-bind="validationElement:PresentAddress.DistrictId">
                                                <select class="form-control" id="district" data-bind="
                                                  options:PresentAddress.DistrictList,
                                                  optionsValue: 'Id',
                                                  optionsText:'Name',
                                                  optionsCaption:'--Select--',
                                                  value:PresentAddress.DistrictId
                                                "></select>
                                            </div>

                                        </div>
                                        <div class="row row_design">
                                            <div class="col-sm-4 col-sm-offset-1">
                                                <label for="ThanaId" class="control-label">Thana<span class="required">*</span> :</label>
                                            </div>
                                            <div class="col-sm-6" data-bind="validationElement:PresentAddress.ThanaId">
                                                <select class="form-control" id="thana" data-bind="
                                                  options:PresentAddress.ThanaList,
                                                  optionsValue: 'Id',
                                                  optionsText:'Name',
                                                  optionsCaption:'--Select--',
                                                  value:PresentAddress.ThanaId
                                                "></select>
                                            </div>

                                        </div>
                                        <div class="row row_design">
                                            <div class="col-sm-4 col-sm-offset-1">
                                                <label for="AreaId" class="control-label">Area<span class="required">*</span> :</label>
                                            </div>
                                            <div class="col-sm-6" data-bind="validationElement:PresentAddress.AreaId">
                                                <select class="form-control" id="area" data-bind="
                                                  options:PresentAddress.AreaList,
                                                  optionsValue: 'Id',
                                                  optionsText:'Name',
                                                  optionsCaption:'--Select--',
                                                  value:PresentAddress.AreaId
                                                "></select>
                                            </div>

                                        </div>
                                    </div>

                                    <div class="col-sm-6">
                                        <div class="row row-border">
                                            <div class="col-sm-6 text-center"><h4 style="color:coral;"><u>Parmanent Address</u></h4></div>
                                            <div class="col-sm-6">
                                                <div class="checkbox">
                                                    <label><input type="checkbox" id="checkbox1" data-bind="checked: myValue">Same as Present address </label>
                                                </div>
                                            </div>
                                        </div>
                                        @*<div class="row row_design">

                                                <div class="col-sm-7"></div>
                                            </div>*@
                                        <div class="row row_design">
                                            <div class="col-sm-4 col-sm-offset-1">
                                                <label for="AddressLine1" class="control-label">Address Line1<span class="required">*</span> :</label>
                                            </div>
                                            <div class="col-sm-6" data-bind="validationElement:ParmanentAddress.AddressLine1">
                                                <input data-bind="value:ParmanentAddress.AddressLine1" class="form-control" type="text" id="Address1_parmanent" name="Address" placeholder="Address">
                                            </div>

                                        </div>
                                        <div class="row row_design">
                                            <div class="col-sm-4 col-sm-offset-1">
                                                <label for="AddressLine2" class="control-label">Address Line2<span class="required">*</span> :</label>
                                            </div>
                                            <div class="col-sm-6" data-bind="validationElement:ParmanentAddress.AddressLine2">
                                                <input data-bind="value:ParmanentAddress.AddressLine2" class="form-control" type="text" id="Address2_parmanent" name="Address" placeholder="Address">
                                            </div>

                                        </div>

                                        <div class="row row_design">
                                            <div class="col-sm-4 col-sm-offset-1">
                                                <label for="PostalCode" class="control-label">PostalCode<span class="required">*</span> :</label>
                                            </div>
                                            <div class="col-sm-6" data-bind="validationElement:ParmanentAddress.PostalCode">
                                                <input data-bind="value:ParmanentAddress.PostalCode" class="form-control" type="text" id="PostalCode_parmanent" name="PostalCode" placeholder="PostalCode">
                                            </div>

                                        </div>
                                        <div class="row row_design">
                                            <div class="col-sm-4 col-sm-offset-1">
                                                <label for="DivisionId" class="control-label">Division<span class="required">*</span> :</label>
                                            </div>
                                            <div class="col-sm-6" data-bind="validationElement:ParmanentAddress.DivisionId">
                                                <select class="form-control" id="division_parmanent" data-bind="
                                                  options:ParmanentAddress.DivisionList,
                                                  optionsValue: 'Id',
                                                  optionsText:'Name',
                                                  optionsCaption:'--Select--',
                                                  value:ParmanentAddress.DivisionId
                                                "></select>
                                            </div>

                                        </div>
                                        <div class="row row_design">
                                            <div class="col-sm-4 col-sm-offset-1">
                                                <label for="DistrictId" class="control-label">District<span class="required">*</span> :</label>
                                            </div>
                                            <div class="col-sm-6" data-bind="validationElement:ParmanentAddress.DistrictId">
                                                <select class="form-control" id="district_parmanent" data-bind="
                                                  options:ParmanentAddress.DistrictList,
                                                  optionsValue: 'Id',
                                                  optionsText:'Name',
                                                  optionsCaption:'--Select--',
                                                  value:ParmanentAddress.DistrictId
                                                "></select>
                                            </div>

                                        </div>
                                        <div class="row row_design">
                                            <div class="col-sm-4 col-sm-offset-1">
                                                <label for="ThanaId" class="control-label">Thana<span class="required">*</span> :</label>
                                            </div>
                                            <div class="col-sm-6" data-bind="validationElement:ParmanentAddress.ThanaId">
                                                <select class="form-control" id="thana_parmanent" data-bind="
                                                  options:ParmanentAddress.ThanaList,
                                                  optionsValue: 'Id',
                                                  optionsText:'Name',
                                                  optionsCaption:'--Select--',
                                                  value:ParmanentAddress.ThanaId
                                                "></select>
                                            </div>

                                        </div>
                                        <div class="row row_design">
                                            <div class="col-sm-4 col-sm-offset-1">
                                                <label for="AreaId" class="control-label">Area<span class="required">*</span> :</label>
                                            </div>
                                            <div class="col-sm-6" data-bind="validationElement:ParmanentAddress.AreaId">
                                                <select class="form-control" id="area_parmanent" data-bind="
                                                  options:ParmanentAddress.AreaList,
                                                  optionsValue: 'Id',
                                                  optionsText:'Name',
                                                  optionsCaption:'--Select--',
                                                  value:ParmanentAddress.AreaId
                                                "></select>
                                            </div>

                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="row row_design">
                                            <div class="col-sm-4 col-sm-offset-1">
                                                <label for="PhoneNo" class="control-label">Phone<span class="required">*</span> :</label>
                                            </div>
                                            <div class="col-sm-6" data-bind="validationElement: PhoneNo">
                                                <input type="tel" data-bind="value:PhoneNo" class="form-control" id="PhoneNo" placeholder="Phone no">
                                            </div>
                                        </div>
                                        <div class="row row_design">
                                            <div class="col-sm-4 col-sm-offset-1">
                                                <label for="Email" class="control-label">Email<span class="required">*</span> :</label>
                                            </div>
                                            <div class="col-sm-6" data-bind="validationElement:Email">
                                                <input type="email" data-bind="value: Email" class="form-control" id="Email" placeholder="Enter Email">
                                                <div class="help-block with-errors"></div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="row row-border">
                                            <div class="col-sm-12 text-center"><h4 style="color:coral;"><u>Imergency Contact</u></h4></div>
                                        </div>
                                        <div class="row row_design">
                                            <div class="col-sm-4 col-sm-offset-1">
                                                <label for="ImergencyContactPerson" class="control-label">Contact Person<span class="required">*</span></label>
                                            </div>
                                            <div class="col-sm-6" data-bind="validationElement:EmergencyContactPerson">
                                                <input type="text" data-bind="value:EmergencyContactPerson" class="form-control" id="ImergencyContactPerson" placeholder="Contact Person">
                                            </div>
                                        </div>
                                        <div class="row row_design">
                                            <div class="col-sm-4 col-sm-offset-1">
                                                <label for="ImergencyContactPhone" class="control-label">Phone<span class="required">*</span> :</label>
                                            </div>
                                            <div class="col-sm-6" data-bind="validationElement:EmergencyContactPhone">
