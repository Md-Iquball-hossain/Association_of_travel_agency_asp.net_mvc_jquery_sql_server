                                <form class="form-horizontal" id="form_joining">
                                    <div class="row row_design">
                                        <div class="col-sm-2 col-sm-offset-1">
                                            <label for="GradeStepId" class="control-label">Grade Step<span class="required">*</span> :</label>
                                        </div>
                                        <div class="col-sm-3" data-bind="validationElement:GradeStepId">
