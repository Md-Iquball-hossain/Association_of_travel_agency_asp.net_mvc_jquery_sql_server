                                                <label for="AddressLine1" class="control-label">Address Line1<span class="required">*</span> :</label>
                                            </div>
                                            <div class="col-sm-6" data-bind="validationElement:PresentAddress.AddressLine1">
                                                <input data-bind="value:PresentAddress.AddressLine1" class="form-control" type="text" id="Address1" name="Address" placeholder="Address">
                                            </div>
                                        </div>
                                        <div class="row row_design">
                                            <div class="col-sm-4 col-sm-offset-1">
                                                <label for="AddressLine2" class="control-label">Address Line2<span class="required">*</span> :</label>
                                            </div>
                                            <div class="col-sm-6" data-bind="validationElement:PresentAddress.AddressLine2">
                                                <input data-bind="value:PresentAddress.AddressLine2" class="form-control" type="text" id="Address2" name="Address" placeholder="Address">
                                            </div>

                                        </div>
                                        <div class="row row_design">
                                            <div class="col-sm-4 col-sm-offset-1">
                                                <label for="PostalCode" class="control-label">PostalCode<span class="required">*</span> :</label>
                                            </div>
                                            <div class="col-sm-6" data-bind="validationElement:PresentAddress.PostalCode">
                                                <input data-bind="textInput:PresentAddress.PostalCode" class="form-control" type="text" id="PostalCode" name="PostalCode" placeholder="PostalCode">
                                            </div>

                                        </div>
