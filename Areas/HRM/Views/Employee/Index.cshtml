                            <div>
                                   <table id="jqGridTraining"></table>
                                    <div id="jqControlsTraining"></div>
                                
                            </div>
                        </div>
                            <div id="education" class="tab-pane fade">
                                
                                    <div>
                                        <table id="jqGridEducation"></table>
                                        <div id="jqControlsEducation"></div>
                                    </div>
                               
                            </div>

