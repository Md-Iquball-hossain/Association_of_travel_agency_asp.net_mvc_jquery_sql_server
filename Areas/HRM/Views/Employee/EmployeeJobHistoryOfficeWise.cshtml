<link href="~/Content/customassets/css/Cart.css" rel="stylesheet" />
<script src="~/Scripts/jquery-ui-1.11.4.min.js"></script>
<script src="~/Scripts/moment.js"></script>
<script src="~/Scripts/knockout-date-bindings.js"></script>
<script src="~/Areas/HRM/Scripts/Employee/employeeJobHistoryOfficeWise.js"></script>

@{
    ViewBag.Title = "Employee Job History By Office";
    Layout = "~/Views/Shared/_LayoutjqGrid.cshtml";
}


<div class="panel">
    <br/>
    <div class="panel-body">

        <div id="historyDiv" class="container-fluid">
            <div class="row">
                <div class="col-sm-12">

                    <div class="form-group row row_design">
                        <label class="control-label col-sm-1 col-sm-offset-3"><span class="select">Employees:</span></label>
                        <div class="col-sm-3">
                            <select class="form-control" data-bind="options:EmployeeList, optionsCaption:'--Choose Employees--', value: EmployeeId, optionsText:'Name', optionsValue:'Id'   "></select> @*, event:{change: getReceivedDates}*@
                        </div>
                        <label class="col-sm-2"><span class="select"><b>Offices</b></span></label>
                        <div class="col-sm-2">
                            <select data-bind="options:OfficeList, optionsCaption:'--Choose Office--', value: OfficeId, optionsText:'Name', optionsValue:'Id' "></select> @*, event:{change: getReceivedDates}*@
                        </div>
                        <div class="col-sm-2">
                            <button type="submit" class="btn btn-deepgreen" data-bind="click:Search">Search</button>
                        </div>
                    </div>
                    <br />
                    <br/>
                    <h4>Export Report</h4>
                    <div class="form-group">

                        <a target="_blank" data-bind="attr: { href: Link1, title1: 'PDF' }">PDF</a>
                        <a target="_blank" data-bind="attr: { href: Link2, title2: 'Excel' }">Excel</a>
                        <a target="_blank" data-bind="attr: { href: Link3, title3: 'Word' }">Word</a>

                    </div>
                    <br/>
                    <div>
                        @*#D9E9FF  #5c9ccc*@

                        <div class="form-signin-heading actions-hover">
                            <table class="table table-striped table-bordered">
                                <thead class="headcolor">
                                    <tr>
                                        <th>Index</th>
                                        <th>EmployeeName</th>
                                        <th>DesignationName</th>
                                        <th>OfficeUnitName</th>
                                        <th>OfficeName</th>
                                        <th>FromDate</th>
                                        <th>ToDate</th>
                                    </tr>
                                </thead>
                                <tbody data-bind="foreach:Employees">
                                    <tr>
                                        <td class="center" data-bind="text: $index() + 1"></td>
                                        <td class="center" data-bind="text: EmployeeName"></td>
                                        <td class="center" data-bind="text: DesignationName"></td>
                                        <td class="center" data-bind="text: OfficeUnitName "></td>
                                        <td class="center" data-bind="text: OfficeName "></td>
                                        <td class="center" data-bind="text: moment(FromDate).format('DD/MMM/YYYY')"></td>
                                        <td class="center" data-bind="text: moment(ToDate).format('DD/MMM/YYYY') "></td>
                                    </tr>
                                </tbody>
                            </table>

                        </div>
                    </div>
                </div>
                @*<div class="form-group row row_design">

                    <div id="myModal" class="modal fade" role="dialog">
