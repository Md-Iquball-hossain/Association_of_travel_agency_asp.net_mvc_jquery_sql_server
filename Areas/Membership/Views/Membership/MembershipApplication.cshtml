    <div class="tab-content rm">

        <div class="tab-pane fade active in" id="Basic">

            <br />

            <div class="col-sm-12">


                <div class="panel bodycontent">
                    <br />
                    <div class="body-main-padding">
                        <div class="form-group row row_design">
                            <div class="col-sm-2 col-sm-offset-1">
                                <label class="control-label">Organization Name :</label>
                            </div>
                            <div class="col-sm-3" data-bind="validationElement: NameOfOrganization">
                                <input class="form-control" id="NameOfOrganization" type="text" data-bind="jqAuto: { value: NameOfOrganization, source:GetMemberList, inputProp: 'value', labelProp: 'value', valueProp: 'NameOfOrganization'}" @*data-bind="value:NameOfOrganization"*@ />
                            </div>
                            <div class="col-sm-2">
                                <label class="control-label">Status of Ownership :</label>
                            </div>
                            <div class="col-sm-3" data-bind="validationElement: OwnershipStatus">
                                @* <select class="form-control" id="OwnershipStatus" data-bind="options: OwnershipStatusList, value: OwnershipId, optionsCaption:'Choose One..', optionsValue:'Id', optionsText: 'Name'"><option value="">Choose One..</option></select>*@
                                <select class="form-control" id="OwnershipStatus" data-bind="options:OwnershipStatusList, optionsValue: 'Id', optionsText:'Name', optionsCaption:'--Select--',value:OwnershipStatus">Choose One..</select>

                            </div>

                        </div>
                        <div class="form-group row row_design">

                            <label class="control-label col-sm-2 col-sm-offset-1">Business Category :</label>
                            <div class="col-sm-3">
                                <select multiple="multiple" id="BusinessCategoryList" class="form-control" data-bind="options: BusinessCategoryList, optionsText: 'Name', optionsValue: 'Id', selectedOptions: BusinessCategories , multiselect: { includeSelectAllOption: true }"></select>

                            </div>

                            <label class="control-label col-sm-2">Other Associations :</label>
                            <div class="col-sm-3">
                                <select multiple="multiple" id="companySelection" class="form-control" data-bind="options: AssociationList, optionsText: 'Name', optionsValue: 'Id', selectedOptions: Associations, multiselect: { includeSelectAllOption: true }"></select>

                            </div>



                        </div>

                        <div class="form-group row row_design">
                            <label class="control-label col-sm-2 col-sm-offset-1">Chamber :</label>
                            <div class="col-sm-3">
                                <select multiple="multiple" id="companySelection" class="form-control" data-bind="options: ChamberList, optionsText: 'Name', optionsValue: 'Id', selectedOptions: Chambers, multiselect: { includeSelectAllOption: true }"></select>

                            </div>
                            <label class="control-label col-sm-2">Member Since :</label>
                            <div class="col-sm-3" data-bind="validationElement: FromYear">
                                <select data-bind="options:Years,optionsValue: 'Year',optionsText:'Year',optionsCaption:'-Member Since-', value:FromYear" class="form-control"></select>
                            </div>
                        </div>
                    </div>
                    <br />
                </div>
            </div>


            <div class="col-sm-12">

                <div class="panel bodycontent">
                    <br />
                    <div class="body-main-padding">
                        <div class="form-group row row_design">
                            <div class="col-sm-2 col-sm-offset-1">
                                <label class="control-label">Trade License No :</label>
                            </div>
                            <div class="col-sm-3" data-bind="validationElement: TradeLicenseNo">
                                <input class="form-control" id="TradeLicenseNo" type="text" data-bind="value:TradeLicenseNo" />
                            </div>


