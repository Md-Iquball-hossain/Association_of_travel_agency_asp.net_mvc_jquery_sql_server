
<script src="~/Scripts/jquery-ui-1.11.4.min.js"></script>
<script src="~/Scripts/moment.js"></script>
<script src="~/Scripts/knockout-date-bindings.js"></script>
<script src="~/Areas/HRM/Scripts/LeaveApplication/leaveAppReportOfficeWise.js"></script>
<link href="~/Content/customassets/css/Cart.css" rel="stylesheet" />

@{
    ViewBag.Title = "Leave Application Information";
    Layout = "~/Views/Shared/_LayoutjqGrid.cshtml";
}




<div class="panel rm">
    <br />
    <div class="panel-body">
        <div class="container-fluid ">

            <div id="receiveDiv">

                <div class="form-group row row_design">

                    <label class="control-label col-sm-1 col-sm-offset-3"><span class="select">Offices :</span></label>
                    <div class="col-sm-3">
                        <select class="form-control" data-bind="options:OfficeList, optionsCaption:'--Choose Offices--', value: officeId, optionsText:'Name', optionsValue:'Id'   "></select> @*, event:{change: getReceivedDates}*@
                    </div>

                    <div class="col-sm-1">
                        <button type="submit" class="btn btn-success" data-bind="click:Search">Search</button>
                    </div>
                </div>
                <br />
                <br />
                <h4>Export Report</h4>
                <div class="form-group">

                    <a target="_blank" data-bind="attr: { href: Link1, title1: 'PDF' }">PDF</a>
                    <a target="_blank" data-bind="attr: { href: Link2, title2: 'Excel' }">Excel</a>
                    <a target="_blank" data-bind="attr: { href: Link3, title3: 'Word' }">Word</a>

                </div>
                <br />

                <table class="table table-striped table-bordered">
                    <thead class="headcolor">
                        <tr>
                            <th>Index</th>
                            <th>Employee Name</th>
                            <th>Office Unit Name</th>
                            <th>Office Name</th>
                            <th>Leave Type</th>
                            <th>Max Entitled Leave Days</th>
                            <th>Leave Taken</th>

                            <th>Remaining Leave</th>

                        </tr>
                    </thead>
                    <tbody data-bind="foreach:Details">
                        <tr>
                            <td class="center" data-bind="text: $index() + 1"></td>
                            <td class="center" data-bind="text: EmployeeName"></td>
                            <td class="center" data-bind="text: OfficeUnitName"></td>
                            <td class="center" data-bind="text: OfficeName "></td>
                            <td class="center" data-bind="text: LeaveTypeName"></td>
                            <td class="center" data-bind="text: EntitledLeaveDays "></td>
                            <td class="center" data-bind="text: TotalDays "></td>
                            <td class="center" data-bind="text: LeaveTaken"></td>
                        </tr>
                    </tbody>
                </table>

            </div>

            <div class="form-group row row_design" ">

                <div id="myModal" class="modal fade" role="dialog">
                    <div class="modal-dialog modal-sm">

                        <!-- Modal content-->
                        <div class="modal-content">
                            <div class="modal-header">
                                <button type="button" class="close" data-dismiss="modal">&times;</button>
                                <h4 class="modal-title">Confirmation</h4>
                            </div>
                            <div class="modal-body" align="center">
                                <p>Do you want to save Receive List?</p>
                                @*<button type="submit" class="btn btn-success" data-bind="click: Submit" data-dismiss="modal">Yes</button>
                                    <button type="button" class="btn btn-success" data-dismiss="modal">No</button>*@

                            </div>
                            <div class="modal-footer">
                            </div>
                        </div>

                    </div>
                </div>

            </div>

            <!-- Success Modal -->
            <div class="form-group row row_design" ">

                <div id="successModal" class="modal fade" role="dialog">
                    <div class="modal-dialog modal-sm">

                        <!-- Modal content-->
                        <div class="modal-content">
                            <div class="modal-header">
                                <button type="button" class="close" data-dismiss="modal">&times;</button>
                                <h4 class="modal-title">Response</h4>
                            </div>
                            <div class="modal-body" align="center">
                                <p id="successModalText"></p>

                                <button type="button" class="btn btn-success" data-dismiss="modal">Ok</button>

                            </div>
                            <div class="modal-footer">
                            </div>
                        </div>

                    </div>
                </div>

            </div>
        </div>
    </div>
</div>




