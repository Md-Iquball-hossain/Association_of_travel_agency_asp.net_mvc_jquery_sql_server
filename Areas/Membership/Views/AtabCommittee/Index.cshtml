@functions {
    public object PhotoName { get; set; }
}
@{
    ViewBag.Title = "Atab Committee";
    Layout = "~/Views/Shared/_LayoutjqGrid.cshtml";
}
<link href="~/Content/bootstrap-datetimepicker.css" rel="stylesheet" />
<script src="~/Scripts/bootstrap-datetimepicker.js"></script>
<script src="~/Scripts/KoCustomDateTimePicker.js"></script>
<script src="~/Scripts/knockout-date-bindings.js"></script>
<link href="~/Content/customassets/css/Cart.css" rel="stylesheet" />
<link href="~/Content/bootstrap-multiselect.css" rel="stylesheet" />
<script src="~/Scripts/bootstrap-multiselect.js"></script>
<script src="~/Areas/Membership/Scripts/AtabCommittee/atabCommittee.js"></script>
<script src="~/Areas/Membership/Scripts/Settings/addressJsViewModel.js"></script>
<script src="~/Scripts/jquery-input-file-text.js"></script>
<script src="~/Scripts/knockout-jqAutocomplete.min.js"></script>
<style>
    .form-control {
        width: 100% !important;
    }

    .btnLarge {
        width: 140px !important;
    }

    .input-group-addon {
        padding: 0 5px;
    }

    .rm {
        background-color: #F8F8F8;
        border-radius: 0px 0px 0px 0px !important;
    }
</style>

<section class="rm">
    <div class="panel-body">
        <div class="container-fluid">
            <div class="row">

                <!---vvvvvvv----Body Main Starts here---vvvvvv----->

                <div class="col-sm-12">
                    <div class="row">
                        <div class="col-sm-12">

                           
                            <div id="DepreciationDiv" enctype="multipart/form-data">
                                <div class="form-group row row_design">
                                    <div class="col-sm-12">
                                        <div class="form-group row row_design">
                                            <label class="control-label col-sm-3 col-sm-offset-2 ">Photo :</label>
                                            <div class="col-sm-2 ">
                                                <img id="cifPhoto" data-bind="attr:{ src:PhotoFile }" style="max-width: 150px; height: auto; display: none;" src="#" alt="Photo Upload" />
                                                @*<img id="cifPhotoOld" data-bind='attr: {src: function(){if (PhotoName()) return "/Content/image/AtabCommittee/"+ PhotoName()} }' style="max-width: 150px; height: auto;" alt="" />*@
                                                <img id="cifPhotoOld" data-bind='attr: {src: "/Content/image/AtabCommittee/"+ PhotoName() }' style="max-width: 150px; height: auto;" alt="" />
                                                <input type='file' id="imgInp" />
                                            </div>
                                        </div>
                                        <div class="form-group row row_design">
                                            <label class="control-label col-sm-3 col-sm-offset-2 " for="FirstName">Agency Name:</label>
                                            <div class="col-sm-5" data-bind="validationElement: FirstName">
                                                <input data-bind="jqAuto: { value: Member, source: GetMemberAutoFill, inputProp: 'value', labelProp: 'value' }" class="form-control" />

                                            </div>
                                        </div>
                                       
                                        <div class="form-group row row_design">
                                            <label class="control-label col-sm-3 col-sm-offset-2 " for="FirstName">First Name :</label>
                                            <div class="col-sm-5" data-bind="validationElement: FirstName">
                                                <input id="FirstName" type="text" data-bind="value:FirstName, valueUpdate: 'keyup'" class="form-control" placeholder="" />
                                            </div>
                                        </div>
                                        <div class="form-group row row_design">
                                            <label class="control-label col-sm-3 col-sm-offset-2" for="LastName">Last Name :</label>
                                            <div class="col-sm-5" data-bind="validationElement: LastName">
                                                <input id="LastName" type="text" data-bind="value:LastName" class="form-control" placeholder="" />
                                            </div>
                                        </div>
                                        <div class="form-group row row_design">
                                            <label class="control-label col-sm-3 col-sm-offset-2" for="Remarks">Remarks :</label>
                                            <div class="col-sm-5" data-bind="validationElement: Remarks">
                                                <input type="text" data-bind="value:Remarks, valueUpdate: 'keyup'" class="form-control" placeholder="" />
                                            </div>
                                        </div>
                                        <div class="form-group row row_design">
                                            <label class="control-label col-sm-3 col-sm-offset-2 "><b>Address:</b></label>

                                        </div>
                                        <div class="form-group row row_design">
                                            <div class="control-label col-sm-3 col-sm-offset-2">
                                                <label for="PermanentAddress.AddressLine1" class="control-label">Address Line 1</label>
                                            </div>
                                            <div class="col-sm-5" data-bind="validationElement: PermanentAddress.AddressLine1">
                                                <input type="text" data-bind="value:PermanentAddress.AddressLine1" class="form-control" id="PermanentAddress.AddressLine1" placeholder="Line 1" readonly/>
                                            </div>
                                        </div>
                                        <div class="form-group row row_design">
                                            <div class="control-label col-sm-3 col-sm-offset-2">
                                                <label for="PermanentAddress.AddressLine2" class="control-label">Address Line 2</label>
                                            </div>
                                            <div class="col-sm-5" data-bind="validationElement: PermanentAddress.AddressLine2">
                                                <input type="text" data-bind="value:PermanentAddress.AddressLine2" class="form-control" placeholder="Line 2" readonly/>
                                            </div>
                                        </div>
                                        <div class="form-group row row_design">
                                            <div class="control-label col-sm-3 col-sm-offset-2">
                                                <label for="PermanentAddress.AddressLine3" class="control-label">Address Line 3</label>
                                            </div>
                                            <div class="col-sm-5" data-bind="validationElement: PermanentAddress.AddressLine3">
                                                <input type="text" data-bind="value:PermanentAddress.AddressLine3" class="form-control" id="PermanentAddress.AddressLine3" placeholder="Line 3" readonly/>

                                            </div>
                                        </div>
                                        <div class="form-group row row_design">
                                            <div class="control-label col-sm-3 col-sm-offset-2">
                                                <label for="PermanentAddress.CountryId" class="control-label">Country<span class="required">*</span></label>
                                            </div>
                                            <div class="col-sm-5" data-bind="validationElement: PermanentAddress.CountryId">
                                                <select class="form-control" id="PermanentAddress.CountryId" data-bind="options:CountryList,optionsValue: 'Id',optionsText:'Name',optionsCaption:'--Select--', value:PermanentAddress.CountryId" disabled></select>
                                            </div>
                                        </div>
                                        <div class="form-group row row_design">
                                            <div class="control-label col-sm-3 col-sm-offset-2">
                                                <label for="PermanentAddress.DivisionId" class="control-label">Division<span class="required">*</span></label>
                                            </div>
                                            <div class="col-sm-5" data-bind="validationElement: PermanentAddress.DivisionId">
                                                <select class="form-control" id="PermanentAddress.DivisionId" data-bind="options:PermanentAddress.DivisionList,optionsValue: 'Id',optionsText:'DivisionNameEng',optionsCaption:'--Select--', value:PermanentAddress.DivisionId" disabled></select>
                                            </div>
                                        </div>
                                        <div class="form-group row row_design">
                                            <div class="control-label col-sm-3 col-sm-offset-2">
                                                <label for="PermanentAddress.DistrictId" class="control-label">District<span class="required">*</span></label>
                                            </div>
                                            <div class="col-sm-5" data-bind="validationElement: PermanentAddress.DistrictId">
                                                <select class="form-control" id="PermanentAddress.DistrictId" data-bind="options:PermanentAddress.DistrictList,optionsValue: 'Id',optionsText:'DistrictNameEng',optionsCaption:'--Select--', value:PermanentAddress.DistrictId" disabled></select>
                                            </div>
                                        </div>
                                        <div class="form-group row row_design">
                                            <div class="control-label col-sm-3 col-sm-offset-2">
                                                <label for="PermanentAddress.ThanaId" class="control-label">Thana<span class="required">*</span></label>
                                            </div>
                                            <div class="col-sm-5" data-bind="validationElement: PermanentAddress.ThanaId">
                                                <select class="form-control" id="PermanentAddress.ThanaId" data-bind="options:PermanentAddress.ThanaList,optionsValue: 'Id',optionsText:'ThanaNameEng',optionsCaption:'--Select--', value:PermanentAddress.ThanaId" disabled></select>
                                            </div>
                                        </div>
                                        <div class="form-group row row_design">
                                            <div class="control-label col-sm-3 col-sm-offset-2">
                                                <label for="PermanentAddress.PostalCode" class="control-label">ZIP/Postal Code</label>
                                            </div>
                                            <div class="col-sm-5" data-bind="validationElement: PermanentAddress.PostalCode">
                                                <input type="number" data-bind="value:PermanentAddress.PostalCode" class="form-control" id="PermanentAddress.PostalCode" placeholder="ZIP/Postal Code" readonly/>
                                            </div>
                                        </div>
                                        <div class="form-group row row_design">
                                            <div class="control-label col-sm-3 col-sm-offset-2">
                                                <label for="PermanentAddress.CellPhoneNo" class="control-label">Mobile</label>
                                            </div>
                                            <div class="col-sm-5" data-bind="validationElement: PermanentAddress.CellPhoneNo">
                                                <input type="number" data-bind="value:PermanentAddress.CellPhoneNo" class="form-control" id="PermanentAddress.CellPhoneNo" placeholder="Enter Mobile Number" readonly/>
                                            </div>
                                        </div>
                                        <div class="form-group row row_design">
                                            <div class="control-label col-sm-3 col-sm-offset-2">
                                                <label for="PermanentAddress.PhoneNo" class="control-label">Phone</label>
                                            </div>
                                            <div class="col-sm-5" data-bind="validationElement:PermanentAddress.PhoneNo">
                                                <input type="number" data-bind="value:PermanentAddress.PhoneNo" class="form-control" id="PermanentAddress.PhoneNo" placeholder="Enter Phone Number" readonly/>
                                            </div>
                                        </div>
                                        <div class="form-group row row_design">
                                            <div class="control-label col-sm-3 col-sm-offset-2">
                                                <label for="PermanentAddress.Email" class="control-label">E-mail</label>
                                            </div>
                                            <div class="col-sm-5" data-bind="validationElement: PermanentAddress.Email">
                                                <input type="email" data-bind="value:PermanentAddress.Email" class="form-control" id="PermanentAddress.Email" placeholder="Enter E-mail"readonly/>
                                            </div>
                                        </div>
                                    </div>
                                    <br />
                                    <div class="form-signin-heading actions-hover">
                                        <div class="col-sm-3">
                                            <label class="control-label"><u><b>Tenure History</b></u><span class="required">*</span></label>
                                        </div>
                                        <table class="table table-striped table-bordered">
                                            <thead class="headcolor">
                                                <tr>
                                                    <th>Zone</th>
                                                    <th>Post</th>
                                                    <th>Committee Type</th>
                                                    <th>Joining Date</th>
                                                    <th>End Of Tenure</th>
                                                    <th>
                                                        <a class="plus" href="#" data-bind="click: addTenures"><span class="glyphicon glyphicon-plus"></span></a>
                                                    </th>
                                                </tr>
                                            </thead>
                                            <tbody data-bind="foreach:TenureHistoryDetails">
                                                <tr>
                                                    <td>
                                                       
                                                        <select class="form-control loan" data-bind="options:$parent.ZoneList, value: Zone, optionsValue:'Id', optionsText: 'Name', optionsCaption: '......Choose......'"></select>
                                                    </td>
                                                    <td>
                                                        <select class="form-control loan" data-bind="options:$parent.PostList, value: Post, optionsValue:'Id', optionsText: 'Name', optionsCaption: '......Choose......'"></select>
                                                    </td>
                                                    <td>
                                                        <select class="form-control loan" data-bind="options:$parent.CommitteeTypes, value: CommitteeType, optionsValue:'Id', optionsText: 'Name', optionsCaption: '......Choose......'"></select>
                                                    </td>
                                                    <td>
                                                        <div class="input-group" style="padding-left: 15px; padding-right: 15px;" data-bind="validationElement: JoiningDate">
                                                            <input type="text" class="centerAlignment form-control lignHeightDate" data-bind="datepicker:JoiningDate, datepickerOptions: {format: 'DD/MM/YYYY'}, valueUpdate:'keyup'">
                                                            <span class="help-block" style="display: none;"></span>
                                                            <div class="input-group-addon">
                                                                <span class="glyphicon glyphicon-calendar"></span>
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <div class="input-group" style="padding-left: 15px; padding-right: 15px;" data-bind="validationElement: EndOfTenure">
                                                            <input type="text" class="centerAlignment form-control lignHeightDate" data-bind="datepicker:EndOfTenure, datepickerOptions: {format: 'DD/MM/YYYY'}, valueUpdate:'keyup'">
                                                            <span class="help-block" style="display: none;"></span>
                                                            <div class="input-group-addon">
                                                                <span class="glyphicon glyphicon-calendar"></span>
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <a class="plus" href='#' data-bind='click: $parent.addTenures'><span class="glyphicon glyphicon-plus"></span></a>
                                                        <a class="minus" href='#' data-bind='click: $parent.removeTenures'><span class="glyphicon glyphicon-minus"></span></a>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                    <div class="form-group row row_design">

                                        <div class="col-sm-offset-8">
                                            <button type="button" id="btnSave" class="btn btn-success" data-bind="enable: IsValid" data-toggle="modal" data-target="#myModal">Save</button> @*, enable :IsValid*@

                                        </div>
                                    </div>
                                    <br />
                                </div>
                                <div class="form-group">
                                    <div id="successModal" class="modal fade" role="dialog">
                                        <div class="modal-dialog modal-sm">
                                            <!-- Modal content-->
                                            <div class="modal-content">
                                                <div class="modal-header">
                                                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                                                    <h4 class="modal-title">Response</h4>
                                                </div>
                                                <div class="modal-body" align="center">
                                                    <p id="successModalText"></p>
                                                    <button type="button" class="btn btn-success" data-dismiss="modal">Ok</button>
                                                </div>
                                                <div class="modal-footer">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div id="myModal" class="modal fade" role="dialog">
                                        <div class="modal-dialog modal-sm">

                                            <!-- Modal content-->
                                            <div class="modal-content">
                                                <div class="modal-header">
                                                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                                                    <h4 class="modal-title">Confirmation</h4>
                                                </div>
                                                <div class="modal-body" align="center">
                                                    <p>Do you want to save comittee?</p>
                                                    <button data-bind="click:Save" class="btn btn-success" data-dismiss="modal">Yes</button>
                                                    <button type="button" class="btn btn-success" data-dismiss="modal">No</button>

                                                </div>
                                                <div class="modal-footer">
                                                    @*<button type="button" class="btn btn-default" data-dismiss="modal">Close</button>*@
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                            <br />

                        </div>
                    </div>
                    @*</section>*@
                    <!---^^^^^^^----Body Main Ends here----^^^^^^^---->
                </div>
            </div>
        </div>
    </div>
</section>



