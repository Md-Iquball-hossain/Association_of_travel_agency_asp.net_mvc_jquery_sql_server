@{
    Layout = "~/Views/Shared/_LayoutjqGrid.cshtml";
    ViewBag.Title = "Balance Sheet Details";
    var CompanyId = ViewBag.CompanyId;
}
@*<link href="~/Content/customStyle.css" rel="stylesheet" />*@
<script src="~/Scripts/moment.js"></script>
<script src="~/Scripts/knockout-date-bindings.js"></script>
<script>
    var userCompanyId = parseInt(@CompanyId);
</script>
<script src="~/Areas/Accounts/Scripts/BalanceSheet/balance_sheet_details.js"></script>
<link href="~/Content/customassets/css/Cart.css" rel="stylesheet" />

<style>
    .form-control {
        width: 100% !important;
    }

    .btnLarge {
        width: 140px !important;
    }

    .input-group-addon {
        padding: 0 5px;
    }

    .rm {
        background-color: #F8F8F8;
        border-radius: 0px 0px 0px 0px !important;
    }
</style>
<section class="rm">
    <div class="panel-body">
        <div class="container-fluid">
            <fieldset>
                <div class="row">
                    <div class="col-sm-12">
                        <!---vvvvvvv----Body Main Starts here---vvvvvv----->
                        <div id="balanceSheetDetails" class="row">


                            <div class="col-sm-12">
                                <section class="panel bodycontent">
                                    <br />
                                    <div class="body-main-padding">
                                        <div class="row">
                                            <div class="form-group row row_design">


                                                @*<label class="col-sm-1">From Date</label>
                                        <div class="col-sm-3">
                                            <input type="date" data-bind="value:FromDate, valueUpdate: 'keyup'" class="form-control">
                                        </div>*@

                                                <label class="control-label col-sm-2 col-sm-offset-1">To Date</label>
                                                <div class="col-sm-3">

                                                    <input type="date" data-bind="value:ToDate, valueUpdate: 'keyup', event:{ change: setUrl}" class="form-control">
                                                </div>

                                                <div class="col-sm-2">
                                                    <button type="submit" data-bind="click:getAllBalanceSheet" class="btn btn-primary">Load</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <br />
                                </section>
                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="form-group row row_design" style="margin: 2px 30px;" align="right">

                                            <div class="PrintReport">
                                                <div class="form-group">
                                                    <h4>Export Report</h4>

                                                    <a target="_blank" data-bind="attr: { href: Link1, title1: 'PDF' }">PDF</a>
                                                    <a data-bind="attr: { href: Link2, title2: 'Excel' }">Excel</a>
                                                    <a data-bind="attr: { href: Link3, title3: 'Word' }">Word</a>

                                                </div>
                                                <br />
                                                <div class="form-group table-responsive">
                                                    <table class="table table-striped table-bordered">
                                                        <thead class="headcolor">
                                                            <tr>
                                                                <th>Account Group</th>
                                                                <th>Account Sub Group</th>
                                                                <th>Account Head Group</th>
                                                                <th>Account Head Sub Group</th>
                                                                <th>Account Head</th>
                                                                <th>Current Year Amount</th>
                                                                <th>Previous Year Amount</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody data-bind="foreach:Employees">
                                                            <tr>
                                                                <td><span data-bind="text: AccGroupName"></span></td>
                                                                <td><span data-bind="text: AccSubGroupName"></span></td>
                                                                <td><span data-bind="text: AccHeadGroupName"></span></td>
                                                                <td><span data-bind="text: AccHeadSubGroupName"></span></td>
                                                                <td><span data-bind="text: AccHeadName"></span></td>
                                                                <td><span data-bind="text: CurrentYearAmount"></span></td>
                                                                <td><span data-bind="text: PreviousYearAmount"></span></td>
                                                            </tr>
                                                        </tbody>
                                                        <tfoot class="headcolor">
                                                            <tr>
                                                                <th>Account Group</th>
                                                                <th>Account Sub Group</th>
                                                                <th>Account Head Group</th>
                                                                <th>Account Head Sub Group</th>
                                                                <th>Account Head</th>
                                                                <th>Current Year Amount</th>
                                                                <th>Previous Year Amount</th>
                                                            </tr>

                                                        </tfoot>
                                                    </table>

                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                </div>

                            </div>

                            <!---^^^^^^^----Body Main Ends here----^^^^^^^---->


                        </div>
                    </div>
                </div>

            </fieldset>
        </div>
    </div>
</section>







