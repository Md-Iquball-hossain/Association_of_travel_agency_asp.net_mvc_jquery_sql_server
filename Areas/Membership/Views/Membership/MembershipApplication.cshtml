        <div class="tab-pane fade" id="Owner">

            <br />
            <div class="form-group row row_design">
                <div class="col-sm-2 col-sm-offset-1">
                    <label class="control-label">Owner Name :</label>
                </div>
                <div class="col-sm-3" data-bind="validationElement:Owner.Name">
                    <input class="form-control" type="text" data-bind="value:Owner.Name" />
                </div>
                <div class="col-sm-2">
                    <label class="control-label">Designation :</label>
                </div>
                <div class="col-sm-3" data-bind="validationElement: Owner.Designation">
                    <input class="form-control" type="text" data-bind="value:Owner.Designation" />
                </div>

            </div>
            <div class="form-group row row_design">
                <div class="col-sm-2 col-sm-offset-1">
                    <label class="control-label">NID :</label>
                </div>
                <div class="col-sm-3" data-bind="validationElement: Owner.NID">
                    <input class="form-control" type="text" data-bind="value:Owner.NID" />
                </div>
                <div class="col-sm-2">
                    <label class="control-label">Phone :</label>
                </div>
                <div class="col-sm-3" data-bind="validationElement: Owner.Phone">
                    <input class="form-control" type="tel" data-bind="value:Owner.Phone" />
                </div>


            </div>
            <div class="form-group row row_design">
                <div class="col-sm-2 col-sm-offset-1">
                    <label class="control-label">Mobile :</label>

                </div>
                <div class="col-sm-3" data-bind="validationElement: Owner.MobileNo">
                    <input class="form-control" type="tel" data-bind="value:Owner.MobileNo" />
                </div>
                <div class="col-sm-2">
                    <label for="Email" class="control-label">Email :@*<span class="required">*</span>*@</label>
                </div>
                <div class="col-sm-3" data-bind="validationElement: Owner.Email">
                    <input type="email" data-bind="value: Owner.Email" class="form-control" id="Email" placeholder="Enter Email">

                </div>

            </div>


            <div class="form-group row row_design">
                <div class="col-sm-2 col-sm-offset-1">
                    <label class="control-label">Present address :</label>

                </div>
                <div class="col-sm-3" @*data-bind="validationElement: DMDate"*@>
                    <textarea class="form-control" data-bind="value: Owner.PresentAddress"></textarea>
                </div>

                <div class="col-sm-2">
                    <label class="control-label">Parmanent address :</label>

                </div>
                <div class="col-sm-3" @*data-bind="validationElement: DMDate"*@>
                    <textarea class="form-control" data-bind="value: Owner.PermanentAddress"></textarea>
                </div>
            </div>

            <div class="form-group row row_design">

                <div class="col-sm-2 col-sm-offset-1">
                    <label class="control-label">Upload Photo :</label>
                </div>
               
                <div class="col-sm-3">
                    <input type="file" name="filePhoto" id="Photo" data-bind="value:Owner.Photo" class="makethispretty" />
                    @*<a data-bind="attr:{href:  $root.Owner.getEditUrlPhoto($data) }, text: Owner.PhotoTest"> </a>*@
                    <a target="_blank" data-bind="attr:{href:  Owner.PhotoPath , click: function() { return true;}, clickBubble: false}, text: Owner.PhotoTest"> </a>
                </div>
              
                <div class="col-sm-3 col-sm-offset-2">
                    <div class="checkbox">
                        <label><input type="checkbox" id="addressCheckbox" data-bind="checked:AddressCheck">Same as Present address </label>
                    </div>
                </div>

            </div>

            <br />

            <div class="button col-sm-offset-9">
                <button type="submit" data-bind="click:saveOwner" class="btn btn-success">Submit</button>
                @*<button type="reset" class="btn btn-danger">Clean</button>*@
            </div>


            <br />


            <hr class="horizontal">
            <div class="row">

                <div class="col-sm-10 col-sm-offset-1">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered">
                            <thead class="headcolor">
                                <tr>
                                    <th class="center">Owner Name</th>
                                    <th class="center">Designation</th>

                                    <th class="center">NID</th>
                                    <th class="center">Phone</th>
                                    <th class="center">Mobile</th>
                                    <th class="center">Email</th>
                                    <th class="center">Address</th>
                                    <th class="center">Photo</th>
                                    <th class="center">Action</th>
                                </tr>
                            </thead>
                            <tbody data-bind="foreach:OwnerList">
                                <tr>
                                    <td style="display:none"><input type="hidden" data-bind="value:Id" /></td>
                                    <td class="center" data-bind="text: Name"></td>
                                    <td class="center" data-bind="text: Designation"></td>

                                    <td class="center" data-bind="text: NID"></td>
                                    <td class="center" data-bind="text: Phone"></td>
                                    <td class="center" data-bind="text: MobileNo"></td>
                                    <td class="center" data-bind="text: Email"></td>
                                    <td class="center" data-bind="text: PresentAddress"></td>
                                   
                                    <td>
                                        @*<a data-bind="attr:{href:  $root.getEditUrlPhotoName(PhotoPath) }, text: PhotoTest"> </a>*@ @*Commented by Maruf*@
                                         @*Code by Maruf*@
                                        <a target="_blank" data-bind="attr:{href: PhotoPath , click: function() { return true;}, clickBubble: false}"> 
                                            <img id="" class="img-rounded" data-bind='attr: {src: PhotoPath }' @*alt="~/UploadedFiles/OwnerPhoto.jpg"*@ width="100" height="100"/>
                                        </a> 
                                        @*Code by Maruf*@
                                    </td>
                                    <td>
                                        <button><span class="glyphicon glyphicon-minus" style="color:red" data-bind="click:$parent.removeOwner" title="Remove"></span></button>
                                        <button><span class="glyphicon glyphicon-pencil" data-bind="click:$parent.EditOwner" title="Edit"></span></button>
                                        
                                    </td>
                                   
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <div class="pagination-centered">
                        @*Page @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber) of @Model.PageCount
