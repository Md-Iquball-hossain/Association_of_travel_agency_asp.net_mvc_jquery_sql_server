                            </div>
                            <div class="col-sm-2">
                                <label class="control-label">Upload File:</label>
                            </div>

                            <div class="col-sm-3">
                                <input type="file" name="fileTradeLicenseFile" id="TradeLicenseFile" data-bind="value:TradeLicenseFile, enable:Id() > 0" class="makethispretty" />
                                @*<a data-bind="attr:{href:  $root.getEditUrlTradeLicenseFile($data) }, text: TradeLicenseFileTest"> </a>*@
                                <a target="_blank" data-bind="attr:{href:  TradeLicenseFilePath , click: function() { return true;}, clickBubble: false}, text: TradeLicenseFileTest"> </a>
                            </div>

                        </div>
                    </div>
                    <br />
                </div>
            </div>


            <div class="col-sm-12">
                <div class="panel bodycontent">
                    <br />
                    <div class="body-main-padding">

                        <div class="form-group row row_design">
                            <div class="col-sm-5 col-sm-offset-1">
                                <label class="control-label">Registration of Civil Aviation & Tourism No :</label>
                            </div>
                            <div class="col-sm-5" data-bind="validationElement: RenewalNo">
                                <input class="form-control" id="renewalNo" type="text" data-bind="value:RenewalNo" />
