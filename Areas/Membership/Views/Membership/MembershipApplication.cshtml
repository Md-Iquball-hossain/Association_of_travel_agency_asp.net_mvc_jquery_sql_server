                                    <input type="text" id="LicenseDate" data-bind="datepicker:LicenseDate, datepickerOptions: {format: 'DD/MM/YYYY'}, valueUpdate: 'keyup'" class="form-control" />
                                    <span class="help-block" style="display: none;"></span>
                                    <div class="input-group-addon">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </div>
                                </div>
                            </div>

                        </div>

                        <div class="form-group row row_design">
                            <div class="col-sm-2  col-sm-offset-1">
                                <label class="control-label">Zone :</label>
                            </div>
                            <div class="col-sm-3" data-bind="validationElement: Zone">
                                @*<select class="form-control" data-bind="options: ZoneList, value: ZoneId, optionsCaption:'Choose One..', optionsValue:'Id', optionsText: 'Name'"><option value="">Choose One..</option></select>*@
