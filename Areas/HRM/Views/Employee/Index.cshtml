@{
    ViewBag.Title = "Employee Information";
    Layout = "~/Views/Shared/_LayoutjqGrid.cshtml";
}
<style>
    .form-control {
        width: 100% !important;
    }

    .btnLarge {
        width: 140px !important;
    }

    .input-group-addon {
        padding: 0 5px;
    }

    .rm {
        background-color: #F8F8F8;
        border-radius: 0px 0px 0px 0px !important;
    }
</style>

<div>
    <table id="jqGrid"></table>
    <div id="jqControls"></div>

</div>
<div id="dismiss" class="container">
    <div class="modal fade" id="myModal" role="dialog">
        <div class="modal-dialog modal-lg">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header" style="padding:0px 0px;">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <ul class="nav nav-tabs nav-justified" role="tablist" style="background-color:#F8F8F8;">

                        <li class="active"><a href="#personal" role="tab" data-toggle="tab"><span class="glyphicon glyphicon-user"></span>Basic Information</a></li>
                        <li><a href="#address" role="tab" data-toggle="tab"><span class="glyphicon glyphicon-envelope"></span>Contact Information</a></li>
                        <li><a href="#education" role="tab" data-toggle="tab"><span class="glyphicon glyphicon-education"></span>Education Information</a></li>
                        <li><a href="#training" role="tab" data-toggle="tab"><span class="glyphicon glyphicon-certificate"></span>Training Information</a></li>
                        <li><a href="#employmenthistory" role="tab" data-toggle="tab"><span class="glyphicon glyphicon-certificate"></span>Employment History</a></li>
                        <li><a href="#joining" role="tab" data-toggle="tab"><span class="glyphicon glyphicon-briefcase"></span>Joining Information</a></li>
                        <li><a href="#userinfo" role="tab" data-toggle="tab"><span class="glyphicon glyphicon-lock"></span>User Information</a></li>
                    </ul>
                </div>
                <div class="modal-body" style="padding:50px 20px 80px 20px;">
                    <div class="tab-content">

                        <div class="tab-pane fade active in" id="personal">
                            <br/>
                            <form class="form-horizontal" role="form" id="employee-info">

                                <div class="form-group row row_design">

                                    <div class="col-sm-2 col-sm-offset-1">
                                        <label for="FirstName" class="control-label">First Name<span class="required">*</span> :</label>
                                    </div>
                                    <div class="col-sm-3" data-bind="validationElement: FirstName">
                                        <input type="text" data-bind="value:FirstName" class="form-control" id="FirstName" placeholder="First Name">
                                    </div>
                                   
                                    <div class="col-sm-2">
                                        <label for="LastName" class="control-label">Last Name<span class="required">*</span> :</label>
                                    </div>
                                    <div class="col-sm-3" data-bind="validationElement: LastName">
                                        <input type="text" data-bind="value:LastName" class="form-control" id="LastName" placeholder="Last Name">
                                    </div>
                                   
                                </div>
                               
                                <div class="form-group row row_design">

                                    <div class="col-sm-2 col-sm-offset-1">
                                        <label for="FatherFirstName" class="control-label">Father FirstName<span class="required">*</span> :</label>
                                    </div>
                                    <div class="col-sm-3" data-bind="validationElement: FatherFirstName">
                                        <input data-bind="value:FatherFirstName" class="form-control" type="text" placeholder="Father FirstName">
                                    </div>                                   

                                    <div class="col-sm-2">
                                        <label for="FatherLastName" class="control-label">Father LastName<span class="required">*</span> :</label>
                                    </div>
                                    <div class="col-sm-3" data-bind="validationElement: FatherLastName">
                                        <input type="text" data-bind="value:FatherLastName" class="form-control" id="FatherLastName" placeholder="Father LastName">
                                    </div>
                           
                                </div>
                                <div class="form-group row row_design">
                                    <div class="col-sm-2 col-sm-offset-1">
                                        <label for="MotherFirstName" class="control-label">MotherFirstName<span class="required">*</span> :</label>
                                    </div>
                                    <div class="col-sm-3" data-bind="validationElement: MotherFirstName">
                                        <input data-bind="value:MotherFirstName" class="form-control" type="text" placeholder="First Name">
                                    </div>
                                  
                                    <div class="col-sm-2">
                                        <label for="MotherLastName" class="control-label">MotherLastName<span class="required">*</span> :</label>
                                    </div>
                                    <div class="col-sm-3" data-bind="validationElement: MotherLastName">
                                        <input data-bind="value:MotherLastName" class="form-control" type="text" placeholder="Last Name">
                                    </div>

                                </div>
                                <div class="form-group row row_design">

                                    <div class="col-sm-2 col-sm-offset-1">
                                        <label for="DateOfBirth" class="control-label">Date of Birth<span class="required">*</span> :</label>
                                    </div>
                                    <div class="col-sm-3" data-bind="validationElement: DateOfBirth">
                                        <input data-bind="date:DateOfBirth" id="DateOfBirthtext" class="form-control dob" type="date" placeholder="Date of Birth">
                                    </div>

                                    <div class="col-sm-2">
                                        <label for="Gender" class="control-label">Gender<span class="required">*</span> :</label>
                                    </div>
                                    <div class="col-sm-3" data-bind="validationElement: Gender">
                                        <select class="form-control" id="gender" data-bind="
                                                  options:GenderList,
                                                  optionsValue: 'Key',
                                                  optionsText:'Value',
                                                  optionsCaption:'--Select--',
                                                  value:Gender
                                                "></select>
                                    </div>
                               
                                </div>
                                <div class="form-group row row_design">

                                    <div class="col-sm-2 col-sm-offset-1">
                                        <label for="MaritalStatus" class="control-label">Marital Status:<span class="required">*</span> :</label>
                                    </div>
                                    <div class="col-sm-3" data-bind="validationElement: MaritalStatus">
                                        <select class="form-control" id="maritalstatus" data-bind="
                                                  options:MaritalStatusList,
                                                  optionsValue: 'Key',
                                                  optionsText:'Value',
                                                  optionsCaption:'--Select--',
                                                  value:MaritalStatus
                                                "></select>
                                    </div>

                                    <div class="col-sm-2">
                                        <label for="BloodGroup" class="control-label">Blood group<span class="required">*</span> :</label>
                                    </div>
                                    <div class="col-sm-3" data-bind="validationElement: BloodGroup">
                                        <select class="form-control" id="bloodgroup" data-bind="options:BloodGroupList,
                                                  optionsValue: 'Key',
                                                  optionsText:'Value',
                                                  optionsCaption:'--Select--',
                                                  value:BloodGroup"></select>
                                    </div>



                                </div>
                                <div class="form-group row row_design">


                                    <div class="col-sm-2 col-sm-offset-1">
                                        <label for="CountryId" class="control-label">Nationality<span class="required">*</span> :</label>
                                    </div>
                                    <div class="col-sm-3" data-bind="validationElement: CountryId">
                                        <select class="form-control" id="nationality" data-bind="options:NationalityList, optionsValue:'Id', optionsText:'Name',optionsCaption:'--Select--',value:CountryId"></select>
                                    </div>


                                    <div class="col-sm-2">
                                        <label for="NID" class="control-label">NID#<span class="required">*</span> :</label>
                                    </div>
                                    <div class="col-sm-3" data-bind="validationElement: NID">
                                        <input type="text" data-bind="value:NID" class="form-control" id="NID" placeholder="Enter NID">
                                    </div>

                                </div>
                                <div class="form-group row row_design">
                                    
                                    <div class="col-sm-2 col-sm-offset-1">
                                        <label for="Photo" class="control-label">Photo<span class="required">*</span> :</label>
                                    </div>
                                    <div class="col-sm-3">
                                        <input class="form-control" type="file" id="file" data-bind="fileInput: Photo, customFileInput: {
                                                      buttonClass: 'btn btn-success',
                                                      fileNameClass: 'disabled form-control'
                                                }" accept="image/*" />
                                    </div>
                                    <div class="col-sm-2">
                                        <label for="EmpCode" class="control-label">Emp Code<span class="required">*</span> :</label>
                                    </div>
                                    <div class="col-sm-3" data-bind="validationElement: EmpCode">
                                        <input data-bind="value:EmpCode" class="form-control" type="text" placeholder="Emp Code" readonly="">
                                    </div>
                                    
                                </div>
                                <br/>
                                <div class="form-group row row_design">
                                    <div class="col-sm-2 col-sm-offset-9">
                                        <button type="submit" data-bind="click:Submit" class="btn btn-success">Submit</button>
                                        <button type="reset" class="btn btn-danger">Clean</button>
                                    </div>
                                   
                                </div>
                            </form>
                        </div>
                        <div class="tab-pane fade" id="address">
                            <br/>
                            <form class="form-horizontal" @*style="padding: 16px 25px 20px 25px !important;"*@ role="form">
